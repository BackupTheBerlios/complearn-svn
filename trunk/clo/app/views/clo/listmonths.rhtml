<div id="info">
<h3> Please choose which month you would like to browse: </h3>
<table border=2 border-style='groove' bordercolor='#705080'>
<% @months.each { |m| %>
<tr><td>
<%= link_to "#{m.strftime("%B %Y")}", :controller => 'clo', :action => 'showmonth', :id => m.to_i  %>
</td></tr>
<% } %>
</table>
