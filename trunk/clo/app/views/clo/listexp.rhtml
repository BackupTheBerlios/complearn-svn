<html>
<head>
<title>
CompLearn Online - All Experiments
</title>
    <link href="/stylesheets/clo.css" media="screen" rel="Stylesheet">
</head>
<body>
<h1>CompLearn Online - All Experiments</h1>
<div id="navbar"> <%= render_partial 'navigation' %> </div>
<table>
<% @expnums.each_with_index { |str, i| %>
<tr><td>
<%=  link_to "Exp-##{(i+1)}:", :action=>'showexpnum', :id=>str, :params=>{"deltime"=>1000, "nonce"=>rand(111)} %>
</td><td> <em> <%= @expterms[i] %> </em>
</tr>
<% } %>
</table>
</body>
</html>
